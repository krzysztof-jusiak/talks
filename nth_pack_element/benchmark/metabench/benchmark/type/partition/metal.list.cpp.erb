#include <metal.hpp>

template<typename N>
using is_even = metal::number<N::type::value % 2 == 0>;

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = metal::list<
    <%= ((k*n+1)..(k*n+n)).map { |i| "metal::number<#{i}>" }.join(', ') %>
>;

#if defined(METABENCH)
using <%= "result#{k}" %> = metal::partition<<%= "xs#{k}" %>, metal::lambda<is_even>>;
#endif

<% end %>

int main() {}

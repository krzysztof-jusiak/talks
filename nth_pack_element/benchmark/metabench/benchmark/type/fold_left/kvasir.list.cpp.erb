#include <kvasir/mpl/mpl.hpp>
namespace mpl = kvasir::mpl;

template<typename, typename X>
using f = X;

struct state;

template<int> struct x;

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = mpl::list<
    <%= ((k*n+1)..(k*n+n)).map { |i| "x<#{i}>" }.join(', ') %>
>;

#if defined(METABENCH)
using <%= "result#{k}" %> = mpl::eager::fold_left<<%= "xs#{k}" %>, state, f>;
#endif

<% end %>


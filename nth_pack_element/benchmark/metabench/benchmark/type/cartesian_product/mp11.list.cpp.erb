#include <boost/mp11.hpp>
using namespace boost::mp11;

template<int> struct x;
template<int> struct y;

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = mp_list<<%= ((k*n+1)..(k*n+n)).map { |i| "x<#{i}>" }.join(', ') %>>;
using <%= "ys#{k}" %> = mp_list<<%= ((k*n+1)..(k*n+n)).map { |i| "y<#{i}>" }.join(', ') %>>;

#if defined(METABENCH)
using <%= "result#{k}" %> = mp_product<mp_list, <%= "xs#{k}" %>, <%= "ys#{k}" %>>;
#endif

<% end %>

int main() {}
